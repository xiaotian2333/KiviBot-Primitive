{
  "name": "keli",
  "version": "1.2.5",
  "packageManager": "pnpm@7.26.3",
  "update": "2023/2/15",
  "description": "Just run „ÅÆ Bot, based on Node.js & movo",
  "main": "lib/index.js",
  "author": "Viki <hi@viki.moe> (https://github.com/vikiboss)",
  "license": "MPL-2.0",
  "bin": {
    "keli": "./bin/keli.js"
  },
  "scripts": {
    "dev": "npm run clean && tsc && concurrently \"tsc -w\" \"tsc-alias -w\"",
    "build": "npm run build:dev",
    "build:dev": "npm run clean && tsc && tsc-alias",
    "build:prod": "npm run clean && tsc && tsc-alias && npm run obfuscate",
    "obfuscate": "node --no-warnings scripts/obfuscate.mjs",
    "release": "npm run build:prod && npm publish",
    "lint": "eslint --ext .js,ts,jsx,tsx .",
    "lint:fix": "eslint --fix --ext .js,ts,jsx,tsx .",
    "clean": "rimraf lib"
  },
  "files": [
    "bin",
    "lib"
  ],
  "engines": {
    "node": ">= v14"
  },
  "dependencies": {
    "dayjs": "^1.11.7",
    "fast-glob": "^3.2.12",
    "fs-extra": "^10.1.0",
    "import-local": "^3.1.0",
    "minimist": "^1.2.8",
    "movo": "1.1.9",
    "nanoid": "^3.3.4",
    "node-cron": "^3.0.2",
    "npm-check-updates": "^16.7.4",
    "ora": "^5.4.1",
    "prompts": "^2.4.2",
    "string2argv": "^1.0.2"
  },
  "devDependencies": {
    "@types/eslint": "^8.21.1",
    "@types/fs-extra": "^9.0.13",
    "@types/minimist": "^1.2.2",
    "@types/node": "^18.13.0",
    "@types/node-cron": "^3.0.7",
    "@types/prompts": "^2.4.2",
    "@vikiboss/prettier-config": "^0.2.2",
    "axios": "^1.3.3",
    "chalk": "^5.2.0",
    "concurrently": "^7.6.0",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-viki-ts": "^0.1.1",
    "eslint-plugin-prettier": "^4.2.1",
    "javascript-obfuscator": "^4.0.2",
    "log4js": "^6.7.1",
    "prettier": "^2.8.4",
    "rimraf": "^3.0.2",
    "tsc-alias": "^1.8.2",
    "typescript": "^4.9.5"
  }
}
